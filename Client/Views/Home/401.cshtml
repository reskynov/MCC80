@{
    ViewData["Title"] = "Home Page";
    Layout = "_LayoutError";
}

<div class="error-Page vh-100 d-flex flex-column align-items-start justify-content-center">
    <!-- Main content -->
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-warning"> 401</h2>

            <div class="error-content">
                <h3><i class="fas fa-exclamation-triangle text-warning"></i> Unauthorized.</h3>

                <p>
                    You are not authorized.
                    <br />
                    Meanwhile, you may return to
                    <br />
                    <a href="Account/Login">Login page.</a>
                </p>
            </div>
            <!-- /.error-content -->
        </div>
        <!-- /.error-page -->
    </section>
    <!-- /.content -->
</div>

